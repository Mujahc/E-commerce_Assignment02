Options -MultiViews
Options -Indexes

RewriteEngine On

# TRY to make localhost == localhost/Main/index
RewriteRule ^$ /Person/login [L]

RewriteBase /

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f

RewriteRule ^(.+)$ index.php?url=$1 [QSA,L]
RewriteRule ^()$ index.php?url=$1 [QSA,L]