Options -MultiViews
Options -Indexes

RewriteEngine On

# # TRY to make localhost == localhost/Publication/index
# RewriteRule ^$ /Publication/index [L]

RewriteBase /

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f

RewriteRule ^(.+)$ index.php?url=$1 [QSA,L]
RewriteRule ^()$ index.php?url=$1 [QSA,L]